{
  "name": "t3docs/screenshots",
  "type": "typo3-cms-extension",
  "description": "Provide tools and runner to take scripted screenshots of the TYPO3 CMS.",
  "authors": [
    {
      "name": "TYPO3 CMS Documentation Team",
      "role": "Developer",
      "homepage": "https://typo3.org/community/teams/documentation"
    },
    {
      "name": "The TYPO3 Community",
      "role": "Contributor",
      "homepage": "https://typo3.org/community/"
    }
  ],
  "homepage": "https://github.com/TYPO3-Documentation/t3docs-screenshots",
  "license": "MIT",
  "keywords": [
    "typo3",
    "screenshot"
  ],
  "support": {
    "issues": "https://github.com/TYPO3-Documentation/t3docs-screenshots/issues"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\CMS\\Screenshots\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "TYPO3\\CMS\\Screenshots\\Tests\\Unit\\": "Tests/Unit/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "screenshots"
    }
  }
}
