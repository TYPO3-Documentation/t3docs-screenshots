{
    "name": "typo3/t3docs-screenshots-introduction",
    "description" : "TYPO3 CMS Screenshots Introduction",
    "repositories": {
        "examples": {
            "type": "path",
            "url": "../../public/typo3conf/ext/examples"
        },
        "packages": {
            "type": "path",
            "url": "../../packages/*"
        }
    },
    "require-dev": {
        "t3docs/examples": "@dev"
    },
    "scripts": {
        "setup-distribution": [
            "typo3cms install:setup --no-interaction --database-user-name root --database-user-password root --database-host-name db --admin-user-name admin --admin-password password --site-setup-type no"
        ]
    },
    "extra": []
}
