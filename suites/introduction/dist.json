{
    "name": "t3docs/screenshots-introduction",
    "description" : "TYPO3 Screenshots Introduction",
    "repositories": {
        "introduction": {
            "type": "path",
            "url": "../../public/typo3conf/ext/introduction"
        },
        "packages": {
            "type": "path",
            "url": "../../packages/*"
        }
    },
    "require-dev": {
        "typo3/cms-introduction": "@dev"
    },
    "scripts": {
        "setup-suite": [
            "typo3cms install:setup --no-interaction --database-user-name root --database-user-password root --database-host-name db --admin-user-name admin --admin-password password --site-setup-type no"
        ]
    },
    "extra": []
}
