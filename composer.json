{
	"name": "typo3/t3docs-screenshots",
	"description" : "TYPO3 CMS Screenshots",
	"license": "MIT",
	"config": {
		"platform": {
			"php": "7.4.1"
		},
		"sort-packages": true
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3-Documentation/t3docs-examples"
		},
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3-Documentation/introduction"
		},
		{
			"type": "path",
			"url": "./packages/*"
		}
	],
	"require": {
		"ext-dom": "*",
		"ext-imagick": "*",
		"ext-json": "*",
		"ext-libxml": "*",
		"bk2k/bootstrap-package": "dev-BP_12-0",
		"codeception/codeception": "^4.1.12",
		"codeception/module-webdriver": "^1.1.4",
		"cweagans/composer-patches": "^1.7",
		"helhum/typo3-console": "dev-latest",
		"sebastian/diff": "^4.0",
		"sensiolabs/ansi-to-html": "^1.2",
		"t3docs/examples": "dev-master",
		"typo3/cms-adminpanel": "dev-master as 11.3.1",
		"typo3/cms-backend": "dev-master as 11.3.1",
		"typo3/cms-belog": "dev-master as 11.3.1",
		"typo3/cms-beuser": "dev-master as 11.3.1",
		"typo3/cms-core": "dev-master as 11.3.1",
		"typo3/cms-dashboard": "dev-master as 11.3.1",
		"typo3/cms-extbase": "dev-master as 11.3.1",
		"typo3/cms-extensionmanager": "dev-master as 11.3.1",
		"typo3/cms-felogin": "dev-master as 11.3.1",
		"typo3/cms-filelist": "dev-master as 11.3.1",
		"typo3/cms-filemetadata": "dev-master as 11.3.1",
		"typo3/cms-fluid": "dev-master as 11.3.1",
		"typo3/cms-fluid-styled-content": "dev-master as 11.3.1",
		"typo3/cms-form": "dev-master as 11.3.1",
		"typo3/cms-frontend": "dev-master as 11.3.1",
		"typo3/cms-impexp": "dev-master as 11.3.1",
		"typo3/cms-indexed-search": "dev-master as 11.3.1",
		"typo3/cms-info": "dev-master as 11.3.1",
		"typo3/cms-install": "dev-master as 11.3.1",
		"typo3/cms-introduction": "dev-master",
		"typo3/cms-linkvalidator": "dev-master as 11.3.1",
		"typo3/cms-lowlevel": "dev-master as 11.3.1",
		"typo3/cms-opendocs": "dev-master as 11.3.1",
		"typo3/cms-recordlist": "dev-master as 11.3.1",
		"typo3/cms-recycler": "dev-master as 11.3.1",
		"typo3/cms-redirects": "dev-master as 11.3.1",
		"typo3/cms-reports": "dev-master as 11.3.1",
		"typo3/cms-rte-ckeditor": "dev-master as 11.3.1",
		"typo3/cms-scheduler": "dev-master as 11.3.1",
		"typo3/cms-seo": "dev-master as 11.3.1",
		"typo3/cms-setup": "dev-master as 11.3.1",
		"typo3/cms-styleguide": "dev-master",
		"typo3/cms-sys-note": "dev-master as 11.3.1",
		"typo3/cms-t3editor": "dev-master as 11.3.1",
		"typo3/cms-tstemplate": "dev-master as 11.3.1",
		"typo3/cms-viewpage": "dev-master as 11.3.1",
		"typo3/screenshots": "dev-main",
		"typo3/testing-framework": "^6.9.0"
	},
	"autoload-dev": {
		"psr-4": {
			"Typo3\\CMS\\Screenshots\\Tests\\Unit\\": "packages/screenshots/Tests/Unit/"
		}
	},
	"scripts":{
		"typo3-cms-scripts": [
			"typo3cms install:fixfolderstructure",
			"typo3cms install:generatepackagestates --excluded-extensions bootstrap_package,examples,introduction,styleguide"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	},
	"extra": {
		"patches": {
			"typo3/cms-core": {
				"Dirty: Disable form protection for easy url hopping": "patches/DisableFormProtection.patch"
			}
		}
	}
}
